<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.Sherlock.Light.DarkActionBar">

        <!-- API 14 theme customizations can go here. -->
        <item name="actionBarTabStyle">@style/COE.ActionBarTab.Style</item>
    </style>

    <style name="COE.ActionBarTab.Style" parent="@style/Widget.Sherlock.ActionBar">
        <item name="android:background">@drawable/actionbar_tab_bg</item>
    </style>

</resources>